{
  "name": "jtl/connector-mapping-tables",
  "description": "Creating connector mapping tables easily and dbms independent",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Immanuel Klinkenberg",
      "email": "immanuel.klinkenberg@jtl-software.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "doctrine/dbal": "^2.5",
    "jtl/connector-dbc": "^2.1.8",
    "jtl/connector": ">=4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1",
    "jtl/unit-test": "^0.1.0",
    "phpstan/phpstan": "^0.12.79",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Jtl\\Connector\\MappingTables\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jtl\\Connector\\MappingTables\\": "tests/src/",
      "Jtl\\Connector\\Dbc\\": "vendor/jtl/connector-dbc/tests/src/"
    }
  }
}
