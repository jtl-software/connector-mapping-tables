{
  "name": "jtl/connector-mapping-tables",
  "description": "Creating connector mapping tables easily and dbms independent.",
  "type": "project",
  "authors": [
    {
      "name": "Immanuel Klinkenberg",
      "email": "immanuel.klinkenberg@jtl-software.com"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "git@gitlab.jtl-software.com:connector/cdbc.git",
      "reference": "origin/master"
    }
  ],
  "require": {
    "doctrine/dbal": "^2.5",
    "jtl/connector-dbc": "^0.14.0",
    "jtl/connector": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "jtl\\Connector\\MappingTables\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "jtl\\Connector\\MappingTables\\": "tests/src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "phpunit/dbunit": "^3.0",
    "phpunit/php-code-coverage": "^5.0"
  }
}
